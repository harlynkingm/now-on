{
  "name": "NowOn - Personal News Filter",
  "description": "A new way to see the news that matters to you.",
  "version": "0.0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage", 
    "geolocation",
    "http://pitchfork.com/rss/news/", 
    "http://rss.nytimes.com/services/xml/rss/nyt/HomePage.xml", 
    "https://www.buzzfeed.com/index.xml", 
    "http://feeds.abcnews.com/abcnews/topstories", 
    "http://www.avclub.com/feeds/rss/", 
    "http://www.vox.com/rss/index.xml",
    "http://assets.complex.com/feeds/channels/all.xml"
  ],
  "manifest_version": 2,
  "icons":{
    "128": "images/icon-128.png"
  }
}
